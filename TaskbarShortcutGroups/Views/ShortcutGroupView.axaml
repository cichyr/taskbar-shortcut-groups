<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:TaskbarShortcutGroups.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="TaskbarShortcutGroups.Views.ShortcutGroupView"
        Title="ShortcutGroupView">
        
        <Design.DataContext>
                <viewModels:ShortcutGroupViewModel />
        </Design.DataContext>
        
        <TextBlock Text="{Binding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
        
</Window>
